    RowBox[{"coeffRange_", ",", " ", "nlist_"}], "]"}], " ", ":=", " ", "\n", 
   "\t\t", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"p", "=", 
