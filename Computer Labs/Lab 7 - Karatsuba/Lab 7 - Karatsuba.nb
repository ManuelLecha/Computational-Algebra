  3.857582653412627*^9, 
  3.857582653661384*^9}},ExpressionUUID->"726e4442-e4cd-4708-8bd8-\
c0348a941400"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"AbstractKaratsuba", "[", 
     RowBox[{"P_", ",", "Q_", ",", "1", ",", "f_"}], "]"}], " ", ":=", " ", 
    RowBox[{"P", "*", "Q"}]}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"AbstractKaratsuba", "[", 
   RowBox[{"P_", ",", "Q_", ",", "n_", ",", "f_"}], "]"}], ":=", " ", "\n", 
  "\t", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "m", ",", "A", ",", "B", ",", "C", ",", "P0", ",", "P1", ",", "Q0", ",", 
      "Q1"}], "}"}], ",", " ", "\n", "\t\t", 
    RowBox[{
     RowBox[{"m", " ", "=", " ", 
      RowBox[{"f", "[", "n", "]"}]}], ";", " ", 
     RowBox[{"P0", " ", "=", " ", 
      RowBox[{"FHT", "[", 
       RowBox[{"P", ",", 
        RowBox[{"m", "-", "1"}]}], "]"}]}], ";", " ", 
     RowBox[{"P1", " ", "=", " ", 
      RowBox[{
