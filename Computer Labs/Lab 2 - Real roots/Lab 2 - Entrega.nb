Cell["\<\
Before proceeding with Descartes Rule of Signs and Boudan Fourier\
\[CloseCurlyQuote]s Algorithm, we are going to implement a function V which \
will be indispensable for both algorithms. We are going to call V to the \
function that given a sequence S of integer values, counts the number of \
changes of sign in it.\
\>", "Text",
 CellChangeTimes->{{3.855031165869549*^9, 
  3.855031350064711*^9}},ExpressionUUID->"afa6f9fe-b1e8-44b9-b81a-\
945c93a0962e"],

Cell[BoxData[
 RowBox[{
  RowBox[{"V", "[", "S_", "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"count", " ", "=", " ", "0"}], ",", "i"}], "}"}], ",", " ", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "2"}], ",", " ", 
       RowBox[{"i", " ", "<=", " ", 
        RowBox[{"Length", "[", "L", "]"}]}], ",", " ", 
       RowBox[{"i", "++"}], ",", " ", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"S", "[", 
            RowBox[{"[", 
             RowBox[{"i", "-", "1"}], "]"}], "]"}], "*", 
           RowBox[{"S", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], " ", "==", " ", "0"}], ",", " ", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"S", "[", 
              RowBox[{"[", 
               RowBox[{"i", "-", "1"}], "]"}], "]"}], "<", "0"}], " ", "||", 
            " ", 
            RowBox[{
             RowBox[{"S", "[", 
              RowBox[{"[", "i", "]"}], "]"}], " ", "<", "0"}]}], ",", " ", 
           RowBox[{"count", "++"}]}], "]"}], ",", " ", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"S", "[", 
              RowBox[{"[", 
               RowBox[{"i", "-", "1"}], "]"}], "]"}], "*", 
             RowBox[{"S", "[", 
              RowBox[{"[", "i", "]"}], "]"}]}], " ", "<", " ", "0"}], ",", 
           " ", 
           RowBox[{"count", "++"}]}], "]"}]}], "]"}]}], "]"}], ";", 
     RowBox[{"Return", "[", "count", "]"}]}]}], "]"}]}]], "Code",
 CellChangeTimes->{{3.855028688600209*^9, 3.85502873245991*^9}, {
