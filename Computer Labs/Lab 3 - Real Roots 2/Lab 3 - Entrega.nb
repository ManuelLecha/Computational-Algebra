Cell[BoxData[
 RowBox[{
  RowBox[{"NewtonSum", "[", 
   RowBox[{"f_", ",", "k_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", ",", "coeff", ",", " ", "list", ",", "si"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{
       RowBox[{
       "We", " ", "expect", " ", "an", " ", "input", " ", "of", " ", "the", 
        " ", "form", " ", "p"}], "=", 
       RowBox[{
        RowBox[{
         RowBox[{"1", "+", 
          RowBox[{"x", "^", "2"}], "+"}], "..."}], " ", "and", " ", "k", " ", 
        "a", " ", "natural", " ", "number"}]}], ",", " ", 
      RowBox[{
       RowBox[{
       "the", " ", "output", " ", "is", " ", "a", " ", "list", " ", "of", 
        "\[IndentingNewLine]", "the", " ", "newton", " ", "sums", " ", "up", 
        " ", "to", " ", "the", " ", "k"}], "-", 
       RowBox[{"th", " ", "one"}]}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"coeff", "=", 
      RowBox[{"Reverse", "[", 
       RowBox[{"CoefficientList", "[", 
        RowBox[{
         RowBox[{"f", "[", "x", "]"}], ",", "x"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"n", "=", 
      RowBox[{
       RowBox[{"Length", "[", "coeff", "]"}], "-", "1"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"n", ">=", "1"}], ",", 
       RowBox[{"list", "=", 
        RowBox[{"{", 
         RowBox[{"n", ",", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"coeff", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], "/", 
           RowBox[{"coeff", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}]}], "}"}]}], ",", 
       RowBox[{"Return", "[", "\"\<Not a valid polynomial\>\"", "]"}]}], 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "2"}], ",", 
       RowBox[{"i", "\[LessEqual]", "k"}], ",", " ", 
       RowBox[{"i", "++"}], ",", " ", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"i", "<=", "n"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"si", "=", 
            RowBox[{"i", " ", "*", 
             RowBox[{"coeff", "[", 
              RowBox[{"[", 
               RowBox[{"i", "+", "1"}], "]"}], "]"}]}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"l", "=", "1"}], ",", 
             RowBox[{"l", "<", "i"}], ",", " ", 
             RowBox[{"l", "++"}], ",", " ", 
             RowBox[{"si", "=", 
              RowBox[{"si", "+", 
               RowBox[{
                RowBox[{"list", "[", 
                 RowBox[{"[", 
                  RowBox[{"i", "+", "1", "-", "l"}], "]"}], "]"}], "*", 
                RowBox[{"coeff", "[", 
                 RowBox[{"[", 
                  RowBox[{"l", "+", "1"}], "]"}], "]"}]}]}]}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"AppendTo", "[", 
            RowBox[{"list", ",", 
             RowBox[{
              RowBox[{"-", "si"}], "/", 
              RowBox[{"coeff", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}]}], "]"}]}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"si", "=", "0"}], ";", "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"l", "=", "1"}], ",", 
             RowBox[{"l", "\[LessEqual]", "n"}], ",", " ", 
             RowBox[{"l", "++"}], ",", " ", 
             RowBox[{"si", "=", 
              RowBox[{"si", "+", 
               RowBox[{
                RowBox[{"list", "[", 
                 RowBox[{"[", 
                  RowBox[{"i", "+", "1", "-", "l"}], "]"}], "]"}], "*", 
                RowBox[{"coeff", "[", 
                 RowBox[{"[", 
                  RowBox[{"l", "+", "1"}], "]"}], "]"}]}]}]}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"AppendTo", "[", 
            RowBox[{"list", ",", 
             RowBox[{
              RowBox[{"-", "si"}], "/", 
              RowBox[{"coeff", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}]}], "]"}]}]}], 
         "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "list", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Code",
 CellChangeTimes->{
  3.855983486180944*^9},ExpressionUUID->"1d43a1cd-3516-423e-998b-\
1ea1ef3bac8c"]
