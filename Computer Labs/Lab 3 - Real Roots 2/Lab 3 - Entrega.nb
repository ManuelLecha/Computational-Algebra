Cell[BoxData[
 RowBox[{
  RowBox[{"Hermite", "[", 
   RowBox[{"f_", ",", "g_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "degf", ",", "degg", ",", "coeff", ",", "coefg", ",", "newtonsum", ",", 
      " ", "h", ",", "s", ",", "listh", ",", 
      RowBox[{"q", "=", "0"}], ",", "eigens", ",", 
      RowBox[{"ph", "=", "0"}], ",", " ", 
      RowBox[{"nh", "=", "0"}], ",", "eigenh"}], "}"}], ",", " ", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "The", " ", "input", " ", "is", " ", "expected", " ", "to", " ", "be", 
      " ", "a", " ", "function"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{
      "The", " ", "function", " ", "return", " ", "a", " ", "list", " ", 
       RowBox[{"{", 
        RowBox[{"realroots", ",", " ", "positive", ",", " ", "negative"}], 
        "}"}], " ", "where", " ", "realroots", " ", "is", " ", "the", " ", 
       "number", " ", "of", " ", "diferent", 
       RowBox[{"(", 
        RowBox[{"complex", " ", "and", " ", "real"}], ")"}], " ", "roots"}], 
      ",", " ", 
      RowBox[{
      "positive", " ", "the", " ", "number", " ", "of", " ", 
       "\[IndentingNewLine]", "real", " ", "roots", " ", "that", " ", 
       "evaluate", " ", "positive", " ", "on", " ", "g", " ", "and", " ", 
       "negative", " ", "the", " ", "number", " ", "of", " ", "real", " ", 
       "roots", " ", "taht", " ", "evaluate", " ", "negative", " ", "on", " ",
        "g"}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "We", " ", "first", " ", "compute", " ", "the", " ", "coefficients", " ",
       "of", " ", "the", " ", "matrix", " ", "H"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"coeff", "=", 
      RowBox[{"Reverse", "[", 
       RowBox[{"CoefficientList", "[", 
        RowBox[{
         RowBox[{"f", "[", "x", "]"}], ",", "x"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"degf", "=", 
      RowBox[{
       RowBox[{"Length", "[", "coeff", "]"}], "-", "1"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"coefg", "=", 
      RowBox[{"CoefficientList", "[", 
       RowBox[{
        RowBox[{"g", "[", "x", "]"}], ",", "x"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"degg", "=", 
      RowBox[{
       RowBox[{"Length", "[", "coefg", "]"}], "-", "1"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
        RowBox[{
        "We", " ", "need", " ", "to", " ", "compute", " ", "the", " ", "h_k", 
         " ", "up", " ", "to", " ", "k"}], "=", 
        RowBox[{
         RowBox[{"2", "degf"}], "-", "2"}]}], ",", " ", 
       RowBox[{
       "and", " ", "to", " ", "compute", " ", "h_k", " ", "we", " ", "need", 
        " ", "the", " ", "newton", " ", "sum", " ", "\[IndentingNewLine]", 
        "up", " ", "to", " ", "s_", 
        RowBox[{"(", 
         RowBox[{"k", "+", "m"}], ")"}]}], ",", " ", 
       RowBox[{
        RowBox[{
        "then", " ", "we", " ", "need", " ", "the", " ", "Newton", " ", 
         "sums", " ", "up", " ", "to", " ", "2", "deg", " ", "f"}], "+", 
        RowBox[{"deg", " ", "g"}], "-", "2"}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"newtonsum", "=", 
      RowBox[{"NewtonSum", "[", 
       RowBox[{"f", ",", 
        RowBox[{
         RowBox[{"2", "*", "degf"}], "+", "degg", "-", "2"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Here", " ", "we", " ", "store", " ", "the", " ", "h_k"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"listh", "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", " ", 
        RowBox[{
         RowBox[{"2", "degf"}], "-", "1"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", " ", 
        RowBox[{
         RowBox[{"2", "degf"}], "-", "1"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"listh", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "=", 
         RowBox[{"coefg", ".", 
          RowBox[{"newtonsum", "[", 
           RowBox[{"[", 
            RowBox[{"i", ";;", 
             RowBox[{"i", "+", "degg"}]}], "]"}], "]"}]}]}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "We", " ", "finally", " ", "store", " ", "the", " ", "matrixes", " ", 
       "of", " ", "S", " ", "and", " ", "h"}], "*)"}], "\[IndentingNewLine]", 
     
     RowBox[{"h", "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"degf", ",", "degf"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"s", "=", "h"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "degf"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"j", "=", "1"}], ",", 
         RowBox[{"j", "\[LessEqual]", "degf"}], ",", 
         RowBox[{"j", "++"}], ",", " ", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"h", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "[", 
            RowBox[{"[", "j", "]"}], "]"}], "=", 
           RowBox[{"listh", "[", 
            RowBox[{"[", 
             RowBox[{"i", "+", "j", "-", "1"}], "]"}], "]"}]}], ";", 
          RowBox[{
           RowBox[{
            RowBox[{"s", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "[", 
            RowBox[{"[", "j", "]"}], "]"}], "=", 
           RowBox[{"newtonsum", "[", 
            RowBox[{"[", 
             RowBox[{"i", "+", "j", "-", "1"}], "]"}], "]"}]}]}]}], "]"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "Remember", " ", "that", " ", "the", " ", "number", "  ", "q", " ", 
        "of", " ", "unique", " ", "pairs", " ", "of", " ", "conjugate", " ", 
        "solutions", " ", "was", " ", "N", 
        RowBox[{"(", "S", ")"}]}], ",", " ", "\[IndentingNewLine]", 
       RowBox[{
       "we", " ", "compute", " ", "it", " ", "with", " ", "the", " ", 
        "eigenvalues"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"eigens", "=", 
      RowBox[{"Eigenvalues", "[", "s", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", 
        RowBox[{"Length", "[", "eigens", "]"}]}], ",", " ", 
       RowBox[{"i", "++"}], ",", " ", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"eigens", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "<", "0"}], ",", 
          RowBox[{"q", "=", 
           RowBox[{"q", "+", "1"}]}]}], "]"}], ";"}]}], "\[IndentingNewLine]",
       "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "We", " ", "compute", " ", "using", " ", "the", " ", "same", " ", 
       "procedure", " ", "P", 
       RowBox[{"(", "H", ")"}], " ", "and", " ", "N", 
       RowBox[{"(", "H", ")"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"eigenh", "=", 
      RowBox[{"Eigenvalues", "[", "h", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", 
        RowBox[{"Length", "[", "eigenh", "]"}]}], ",", " ", 
       RowBox[{"i", "++"}], ",", " ", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"eigenh", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "<", "0"}], ",", 
