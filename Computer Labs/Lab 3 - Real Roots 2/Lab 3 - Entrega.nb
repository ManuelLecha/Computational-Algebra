       RowBox[{"x", "^", "2"}], "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"a", "+", "b"}], ")"}], "x"}], "+", 
       RowBox[{"a", "*", "b"}]}]}], ";", " ", 
     RowBox[{"Return", "[", 
      RowBox[{
       RowBox[{"HermiteRes", "[", 
        RowBox[{"f", ",", "g"}], "]"}], "[", 
       RowBox[{"[", "3", "]"}], "]"}], "]"}], ";"}]}], "]"}]}]], "Code",
