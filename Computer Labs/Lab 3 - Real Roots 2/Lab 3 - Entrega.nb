         RowBox[{"k", "+", "m"}], ")"}]}], ",", " ", 
       RowBox[{
        RowBox[{
        "then", " ", "we", " ", "need", " ", "the", " ", "Newton", " ", 
         "sums", " ", "up", " ", "to", " ", "2", "deg", " ", "f"}], "+", 
        RowBox[{"deg", " ", "g"}], "-", "2"}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"newtonsum", "=", 
      RowBox[{"NewtonSumRes", "[", 
       RowBox[{"f", ",", 
        RowBox[{
         RowBox[{"2", "*", "degf"}], "+", "degg", "-", "2"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Here", " ", "we", " ", "store", " ", "the", " ", "h_k"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"listh", "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", " ", 
        RowBox[{
         RowBox[{"2", "degf"}], "-", "1"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", " ", 
        RowBox[{
         RowBox[{"2", "degf"}], "-", "1"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"listh", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "=", 
         RowBox[{"coefg", ".", 
          RowBox[{"newtonsum", "[", 
           RowBox[{"[", 
            RowBox[{"i", ";;", 
             RowBox[{"i", "+", "degg"}]}], "]"}], "]"}]}]}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "We", " ", "finally", " ", "store", " ", "the", " ", "matrixes", " ", 
       "of", " ", "S", " ", "and", " ", "h"}], "*)"}], "\[IndentingNewLine]", 
     
     RowBox[{"h", "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"degf", ",", "degf"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"s", "=", "h"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "degf"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"j", "=", "1"}], ",", 
         RowBox[{"j", "\[LessEqual]", "degf"}], ",", 
         RowBox[{"j", "++"}], ",", " ", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"h", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "[", 
            RowBox[{"[", "j", "]"}], "]"}], "=", 
           RowBox[{"listh", "[", 
            RowBox[{"[", 
             RowBox[{"i", "+", "j", "-", "1"}], "]"}], "]"}]}], ";", 
          RowBox[{
           RowBox[{
            RowBox[{"s", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "[", 
            RowBox[{"[", "j", "]"}], "]"}], "=", 
           RowBox[{"newtonsum", "[", 
            RowBox[{"[", 
             RowBox[{"i", "+", "j", "-", "1"}], "]"}], "]"}]}]}]}], "]"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "Remember", " ", "that", " ", "the", " ", "number", "  ", "q", " ", 
        "of", " ", "unique", " ", "pairs", " ", "of", " ", "conjugate", " ", 
        "solutions", " ", "was", " ", "N", 
        RowBox[{"(", "S", ")"}]}], ",", " ", "\[IndentingNewLine]", 
       RowBox[{
       "we", " ", "compute", " ", "it", " ", "with", " ", "the", " ", 
        "eigenvalues"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"eigens", "=", 
      RowBox[{"Eigenvalues", "[", "s", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", 
        RowBox[{"Length", "[", "eigens", "]"}]}], ",", " ", 
       RowBox[{"i", "++"}], ",", " ", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"eigens", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "<", "0"}], ",", 
          RowBox[{"q", "=", 
           RowBox[{"q", "+", "1"}]}]}], "]"}], ";"}]}], "\[IndentingNewLine]",
       "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "We", " ", "compute", " ", "using", " ", "the", " ", "same", " ", 
       "procedure", " ", "P", 
       RowBox[{"(", "H", ")"}], " ", "and", " ", "N", 
       RowBox[{"(", "H", ")"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"eigenh", "=", 
      RowBox[{"Eigenvalues", "[", "h", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", 
        RowBox[{"Length", "[", "eigenh", "]"}]}], ",", " ", 
       RowBox[{"i", "++"}], ",", " ", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"eigenh", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "<", "0"}], ",", 
          RowBox[{"nh", "=", 
           RowBox[{"nh", "+", "1"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"eigenh", "[", 
            RowBox[{"[", "i", "]"}], "]"}], ">", "0"}], ",", 
          RowBox[{"ph", "=", 
           RowBox[{"ph", "+", "1"}]}]}], "]"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"ph", "+", "nh"}], ",", 
