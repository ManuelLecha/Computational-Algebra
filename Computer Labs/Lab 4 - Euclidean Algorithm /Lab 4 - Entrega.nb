Cell[TextData[{
 StyleBox[" Exercise 5:",
  FontWeight->"Bold"],
 " Modify either of the versions (recursive or iterative) of your code in \
such a way that the output is a relation of the form {r,s,u}, with \
r.a+s.b=u=gcd(a,b)."
}], "Subsection",
 CellChangeTimes->{{3.8560054956514797`*^9, 
  3.856005500873748*^9}},ExpressionUUID->"7fb5a87c-4088-4458-8166-\
85f5e77bb05b"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"IterativeExtendedGCD", "[", 
    RowBox[{"a_", ",", "b_"}], "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"d", "=", "1"}], ",", "t", ",", "f", ",", "g", ",", "f2", ",", 
       "g2", ",", 
       RowBox[{"s1", "=", "0"}], ",", 
       RowBox[{"s2", "=", "1"}], ",", 
       RowBox[{"t1", "=", "1"}], ",", 
       RowBox[{"t2", "=", "0"}]}], "}"}], ",", " ", "\n", 
     RowBox[{
      RowBox[{"f", "=", "a"}], ";", 
      RowBox[{"g", "=", "b"}], ";", "\n", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"EvenQ", "[", "f", "]"}], "&&", 
         RowBox[{"EvenQ", "[", "g", "]"}]}], ",", " ", 
        RowBox[{
         RowBox[{"f", "=", 
          RowBox[{"f", "/", "2"}]}], ";", 
         RowBox[{"g", "=", 
          RowBox[{"g", "/", "2"}]}], ";", 
         RowBox[{"d", "=", 
          RowBox[{"2", "*", "d"}]}]}]}], "]"}], ";", " ", "\n", 
      RowBox[{"f2", "=", "f"}], ";", 
      RowBox[{"g2", "=", "g"}], ";", "\n", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"f", "!=", "0"}], ",", "\n", "\t", 
        RowBox[{
         RowBox[{"While", "[", 
          RowBox[{
           RowBox[{"EvenQ", "[", "f", "]"}], ",", 
           RowBox[{
            RowBox[{"f", "=", 
             RowBox[{"f", "/", "2"}]}], ";", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"EvenQ", "[", "t2", "]"}], "&&", 
               RowBox[{"EvenQ", "[", "s2", "]"}]}], ",", 
              RowBox[{
               RowBox[{"t2", "=", 
                RowBox[{"t2", "/", "2"}]}], ";", 
               RowBox[{"s2", "=", 
                RowBox[{"s2", "/", "2"}]}], ";"}], ",", 
              RowBox[{
               RowBox[{"s2", "=", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"s2", "+", "g2"}], ")"}], "/", "2"}]}], ";", 
               RowBox[{"t2", "=", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"t2", "-", "f2"}], ")"}], "/", "2"}]}]}]}], 
             "]"}]}]}], "]"}], ";", "\n", "\t", 
         RowBox[{"While", "[", 
          RowBox[{
           RowBox[{"EvenQ", "[", "g", "]"}], ",", 
           RowBox[{
            RowBox[{"g", "=", 
             RowBox[{"g", "/", "2"}]}], ";", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"EvenQ", "[", "t1", "]"}], "&&", 
               RowBox[{"EvenQ", "[", "s1", "]"}]}], ",", 
              RowBox[{
               RowBox[{"t1", "=", 
                RowBox[{"t1", "/", "2"}]}], ";", 
               RowBox[{"s1", "=", 
                RowBox[{"s1", "/", "2"}]}], ";"}], ",", 
              RowBox[{
               RowBox[{"s1", "=", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"s1", "+", "g2"}], ")"}], "/", "2"}]}], ";", 
               RowBox[{"t1", "=", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"t1", "-", "f2"}], ")"}], "/", "2"}]}]}]}], 
             "]"}]}]}], "]"}], ";", "\n", "\t", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"f", ">=", "g"}], ",", 
           RowBox[{
            RowBox[{"f", "=", 
             RowBox[{"f", "-", "g"}]}], ";", 
            RowBox[{"s2", "=", 
             RowBox[{"s2", "-", "s1"}]}], ";", 
            RowBox[{"t2", "=", 
             RowBox[{"t2", "-", "t1"}]}]}], ",", 
           RowBox[{
            RowBox[{"g", "=", 
             RowBox[{"g", "-", "f"}]}], ";", 
            RowBox[{"s1", "=", 
             RowBox[{"s1", "-", "s2"}]}], ";", 
            RowBox[{"t1", "=", 
             RowBox[{"t1", "-", "t2"}]}]}]}], "]"}], ";"}]}], "\n", "]"}], 
      ";", "\n", 
      RowBox[{"Return", "[", 
       RowBox[{"{", 
        RowBox[{"s1", ",", "t1", ",", 
         RowBox[{"d", "*", "g"}]}], "}"}], "]"}], "\n", ";"}]}], "]"}]}], 
  "\n"}], "\n", 
 RowBox[{"IterativeExtendedGCD", "[", 
  RowBox[{"36", ",", "42"}], "]"}]}], "Code",
