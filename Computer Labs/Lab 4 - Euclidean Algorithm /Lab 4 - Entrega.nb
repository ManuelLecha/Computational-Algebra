Cell[TextData[{
 StyleBox["Exercise 4: ",
  FontWeight->"Bold"],
 "Design and implement an iterative version of this algorithm."
}], "Subsection",
 CellChangeTimes->{{3.856005443460997*^9, 
  3.8560054602968893`*^9}},ExpressionUUID->"01befb3c-e256-4ca3-a904-\
d22185ac5736"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"IterativeGCD", "[", 
    RowBox[{"a_", ",", "b_"}], "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"d", "=", "1"}], ",", "t", ",", "f", ",", "g"}], "}"}], ",", 
     " ", "\n", 
     RowBox[{
      RowBox[{"f", "=", "a"}], ";", 
      RowBox[{"g", "=", "b"}], ";", "\n", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"EvenQ", "[", "f", "]"}], "&&", 
         RowBox[{"EvenQ", "[", "g", "]"}]}], ",", " ", 
        RowBox[{
         RowBox[{"f", "=", 
          RowBox[{"f", "/", "2"}]}], ";", 
         RowBox[{"g", "=", 
          RowBox[{"g", "/", "2"}]}], ";", 
         RowBox[{"d", "=", 
          RowBox[{"2", "*", "d"}]}]}]}], "]"}], ";", " ", "\n", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"f", "!=", "0"}], ",", "\n", "\t", 
        RowBox[{
         RowBox[{"While", "[", 
          RowBox[{
           RowBox[{"EvenQ", "[", "f", "]"}], ",", 
           RowBox[{"f", "=", 
            RowBox[{"f", "/", "2"}]}]}], "]"}], ";", "\n", "\t", 
         RowBox[{"While", "[", 
          RowBox[{
           RowBox[{"EvenQ", "[", "g", "]"}], ",", 
           RowBox[{"g", "=", 
            RowBox[{"g", "/", "2"}]}]}], "]"}], ";", "\n", "\t", 
         RowBox[{"t", "=", 
          RowBox[{
           RowBox[{"Abs", "[", 
            RowBox[{"f", "-", "g"}], "]"}], "/", "2"}]}], ";", "\n", "\t", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"f", ">=", "g"}], ",", 
           RowBox[{"f", "=", "t"}], ",", 
           RowBox[{"g", "=", "t"}]}], "]"}], ";"}]}], "\n", "]"}], ";", "\n", 
      
      RowBox[{"Return", "[", 
       RowBox[{"d", "*", "g"}], "]"}], "\n", ";"}]}], "]"}]}], "\n"}], "\n", 
 RowBox[{"IterativeGCD", "[", 
  RowBox[{"12", ",", "6"}], "]"}]}], "Code",
 CellChangeTimes->{{3.856005511759734*^9, 3.8560055527942553`*^9}, {
