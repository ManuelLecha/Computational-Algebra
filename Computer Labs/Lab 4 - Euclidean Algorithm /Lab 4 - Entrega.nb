Cell[TextData[{
 StyleBox["Exercise 2: ",
  FontWeight->"Bold"],
 "Design and implement a recursive algorithm which allows the computation of \
the gcd of any pair of integers by using the three properties mentioned \
above. Show that the algorithm concludes in a finite number of steps."
}], "Subsection",
 CellChangeTimes->{{3.856004849957363*^9, 
  3.8560048844934587`*^9}},ExpressionUUID->"bd2831e0-fc78-481f-ab5a-\
ab81afbc11b0"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"RecursiveGCD", "[", 
    RowBox[{"a_", ",", "0"}], "]"}], ":=", " ", "a"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"RecursiveGCD", "[", 
   RowBox[{"a_", ",", "b_"}], "]"}], " ", ":=", " ", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"EvenQ", "[", "a", "]"}], "&&", 
     RowBox[{"EvenQ", "[", "b", "]"}]}], ",", 
    RowBox[{"2", "*", 
     RowBox[{"RecursiveGCD", "[", 
      RowBox[{
       RowBox[{"a", "/", "2"}], ",", 
       RowBox[{"b", "/", "2"}]}], "]"}]}], ",", " ", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"EvenQ", "[", "a", "]"}], ",", 
       RowBox[{"RecursiveGCD", "[", 
        RowBox[{
         RowBox[{"a", "/", "2"}], ",", "b"}], "]"}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"EvenQ", "[", "b", "]"}], ",", 
         RowBox[{"RecursiveGCD", "[", 
          RowBox[{"a", ",", 
           RowBox[{"b", "/", "2"}]}], "]"}]}], "]"}]}], "]"}], ";", " ", 
     RowBox[{"RecursiveGCD", "[", 
      RowBox[{
       RowBox[{"Min", "[", 
        RowBox[{"a", ",", "b"}], "]"}], ",", 
       RowBox[{"Abs", "[", 
        RowBox[{"a", "-", "b"}], "]"}]}], "]"}]}]}], "]"}]}]}], "Code",
 CellChangeTimes->{{3.85600687410643*^9, 3.856006895065084*^9}, {
