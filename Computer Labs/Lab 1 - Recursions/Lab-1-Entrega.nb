Cell[BoxData[
 RowBox[{
  RowBox[{"Iterative", "[", 
   RowBox[{"x_", ",", "n_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"y", "=", "1"}], ",", 
      RowBox[{"base", "=", "x"}]}], "}"}], ",", " ", 
    RowBox[{
     RowBox[{"k", "=", "n"}], ";", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"k", ">=", "1"}], ",", " ", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"OddQ", "[", "k", "]"}], ",", 
          RowBox[{
           RowBox[{"y", "*=", "base"}], ";", 
           RowBox[{"k", "--"}]}]}], "]"}], ";", " ", 
        RowBox[{"base", "=", 
         RowBox[{"base", "^", "2"}]}], ";", " ", 
        RowBox[{"k", "=", 
         RowBox[{"k", "/", "2"}]}], ";"}]}], "]"}], ";", " ", 
     RowBox[{"Return", "[", "y", "]"}]}]}], "]"}]}]], "Code",
