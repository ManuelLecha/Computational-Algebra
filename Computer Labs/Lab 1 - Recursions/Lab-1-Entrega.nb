      RowBox[{"x", "*", "x"}], ",", 
      RowBox[{"n", "/", "2"}]}], "]"}], ",", 
    RowBox[{"x", "*", 
     RowBox[{"RecBin", "[", 
      RowBox[{
       RowBox[{"x", "*", "x"}], ",", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"n", "\[Minus]", "1"}], ")"}], "/", "2"}]}], "]"}]}]}], 
   "]"}]}]}], "Code",
