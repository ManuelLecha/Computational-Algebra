Cell[BoxData[
 RowBox[{
  RowBox[{"IterativeNaive", "[", 
   RowBox[{"x_", ",", "n_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"y", "=", "1"}], "}"}], ",", " ", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", " ", 
       RowBox[{"i", "<=", " ", "n"}], ",", 
       RowBox[{"i", "++"}], ",", 
       RowBox[{"y", "=", 
        RowBox[{"x", "*", "y"}]}]}], "]"}], ";", " ", 
     RowBox[{"Return", "[", "y", "]"}]}]}], "]"}]}]], "Code",
