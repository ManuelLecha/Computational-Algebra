 RowBox[{
  RowBox[{
   RowBox[{"RecBin", "[", 
    RowBox[{"x_", ",", "0"}], "]"}], ":=", "1"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"RecBin", "[", 
    RowBox[{"x_", ",", "1"}], "]"}], ":=", "x"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"RecBin", "[", 
   RowBox[{"x_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"EvenQ", "[", "n", "]"}], ",", 
    RowBox[{"RecBin", "[", 
