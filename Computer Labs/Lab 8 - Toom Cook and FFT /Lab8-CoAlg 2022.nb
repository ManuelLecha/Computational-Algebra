     RowBox[{"R1", "=", 
      RowBox[{"RR", "[", 
       RowBox[{"[", 
        RowBox[{"2", ";;", ";;", "2"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"L0", "=", 
      RowBox[{"FFT", "[", 
       RowBox[{"R0", ",", "a", ",", "m"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"L1", "=", 
      RowBox[{"FFT", "[", 
       RowBox[{"R1", ",", "a", ",", "m"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"L1", "=", 
