          RowBox[{"[", "1", "]"}], "]"}], "}"}], "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"m", "=", 
      RowBox[{"n", "/", "2"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"a", "=", 
      RowBox[{"w", "*", "w"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"R0", "=", 
      RowBox[{"RR", "[", 
       RowBox[{"[", 
        RowBox[{"1", ";;", ";;", "2"}], "]"}], "]"}]}], ";", 
